#include <iostream>
using namespace std;
void ler_vetor(int vetor[], int n){
for(int i=0; i<n; i++){
    cout<<"Insira o corpo do vetor :";
    cin>>vetor[i];
}
}
void imprimir( int  vetor[], int n){
for(int i =0; i<n; i++){
        vetor [i];

}

}
int main(){
int cont =0 , x =0;
int n;
cout<<"fgsdd";
cin>>n;
int vetor[n];
ler_vetor(vetor, n);
for(int x=0; x<10; x++){
        cont=0;
    for(int i=0; i<n; i++){
        if ( vetor[i]==x){
            cont ++;
        }
    }
  cout<<x<<" ocorre "<<cont<<" vezes"<<endl;
}


return 0;
}
