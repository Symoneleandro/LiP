#include<iostream>
using namespace std;
int lervetor(int vetornota[], int n)
{
    int maior;
    for (int i=0; i<n; i++)
    {
        cout<<"Digite os valores do vetor: \n";
        cin>>vetornota[i];
        if (i ==0){
            maior = vetornota[i];
        }
        else{
            if(vetornota[i]>maior){
                maior = vetornota[i];
            }
        }
    }
    return maior;
}

int main(){
 int  n;
    cout<<"Insira a quantidade de vendas:";
    cin>>n;
    int vetor[n];
    cout<<lervetor(vetor,n )<<endl;

    return 0;

}

