#include<iostream>
using namespace std;
void lervetor( int vetor[], int n)
{
    int valor;
    for(int i=0; i<n; i++)
    {
        cout<<"Digite o valor:\n";
        cin>>vetor[i];
    }
}
bool valorigual( int vetor[], int n, int x  )
{
    for(int i=0; i<n; i++)
    {
        if(vetor[i]==x)
        {
            return true;
        }


    }
return false 
}
int main (){
int n, x;
cout<<"Insira o valor de vendas:";
cin>>n;
int vetor[n];
lervetor(vetor,n);
cout<<"venda de referencia:";
cin>>x;
cout<<  valorigual(vetor,n,x )<< endl;
return 0;

}

