#include<iostream>
using namespace std;
void ler_vetor(int vetor[], int n){
for(int i=0;i<n;i++){
    cout<<"insira o corpo do vetor:\n";
    cin>>vetor[i];
}
}
int contpar(int vetor[], int n){
    int cont1=0;
for(int i=0;i<n; i++){
    if( vetor[i]%2==0){
        cont1 ++;
    }
}
}
int contimpar(int vetor[], int n){
    int cont2 =0;
for(int i=0;i<n; i++){
    if( vetor[i]%2==0){
        cont2 ++;
    }
}
}
