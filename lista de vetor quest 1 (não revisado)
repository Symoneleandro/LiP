#include<iostream>
using namespace std;
void lervetor(int vetor[], int n)
{
    for (int i=0; i<n; i++)
    {
        cout<<"Digite os valores do vetor: \n";
        cin>>vetor[i];
    }
}
float mediavetor(int vetor[], int n ){
float soma ;
for (int i=0;i<n; i++){
    soma = soma + vetor[i];
}
return soma;
}

int main(){
int n, soma, media ;
cout<<"Insira o tamanho do vetor\n";
cin>>n;
int vetor[n];
lervetor(vetor,n );
soma = mediavetor(vetor, n);
media= soma/n;
cout<<media<< endl;
return 0;
}

